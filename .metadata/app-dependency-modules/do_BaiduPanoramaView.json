{
  "ID": "do_BaiduPanoramaView",
  "IsContainer": "0",
  "Type": "UI",
  "Name": "百度全景地图",
  "Version": "Internal",
  "Description": "百度全景图是一种实景地图服务。为用户提供城市、街道和其他环境的360度全景图像，用户可以通过该服务获得如临其境的地图浏览体验。全景地图使用新的地图技术，营造新的产品体验。真正实现“人视角”的地图浏览体验，为用户提供更加真实准确、更富画面细节的地图服务",
  "Icon": "https://deviceone.blob.core.chinacloudapi.cn/store/D/20160121/do_BaiduPanoramaView/百度全景地图.jpg",
  "IOS": {
    "Support": 1,
    "MinVersion": "8"
  },
  "Android": {
    "Support": 1,
    "MinVersion": "4.0"
  },
  "Windows": {
    "Support": 0,
    "MinVersion": ""
  },
  "WindowsPhone": {
    "Support": 0,
    "MinVersion": ""
  },
  "Html": {
    "Support": 1,
    "MinVersion": "5.0"
  },
  "Properties": [
    {
      "ID": "imageLevel",
      "Name": "全景图片的显示级别",
      "Type": "Number",
      "DefaultValue": "2",
      "EditType": "1",
      "Group": "",
      "Description": "设置全景图片的显示级别，1为较低清晰度, 2为中等清晰度 , 3为较高清晰度",
      "Design": {
        "Type": "UInteger",
        "Data": []
      }
    },
    {
      "ID": "zoomLevel",
      "Name": "全景图缩放级别",
      "Type": "Number",
      "DefaultValue": "2",
      "EditType": "1",
      "Group": "",
      "Description": "设置全景图的缩放级别，默认缩放级别为2级，缩放级别总共分为5级，分别是1-5级，随着级别的增大清晰度逐渐提高",
      "Design": {
        "Type": "UInteger",
        "Data": []
      }
    }
  ],
  "Events": [
    {
      "ID": "touchMarker",
      "Description": "点击标记时触发",
      "ReturnType": "Node",
      "ReturnDesc": "[{\"id\":\"标记ID,用户自定义标记唯一ID\",\"latitude\":\"纬度\",\"longitude\":\"经度\",\"type\":\"ImageMark|TextMark\",\"info\":\"当type为ImageMark时返回缩略图地址|当type为TextMark时返回文字标注内容\"}]"
    }
  ],
  "syncMethodes": [
    {
      "ID": "addImageMarkers",
      "Name": "添加一组缩略图标记",
      "Description": "将一组经纬度用缩略图在百度地图标记出来,缩略图地址为 data:// source:// 打头的URI格式，不能包含@符号。其中文件格式说明可参考Storage类；iOS8.0以下不支持",
      "ReturnType": "Bool",
      "ReturnDesc": "true 成功 false 失败",
      "Paras": [
        {
          "ID": "data",
          "Name": "数据",
          "Type": "Node",
          "Required": 1,
          "DefaultValue": "",
          "Description": "[{\"id\":\"标记ID,用户自定义标记唯一ID\",\"latitude\":\"纬度\",\"longitude\":\"经度\",\"url\":\"缩略图地址\"},...]，其中id必须唯一"
        }
      ]
    },
    {
      "ID": "addTextMarkers",
      "Name": "添加一组文本标记",
      "Description": "",
      "ReturnType": "Bool",
      "ReturnDesc": "true 成功 false 失败",
      "Paras": [
        {
          "ID": "data",
          "Name": "数据",
          "Type": "Node",
          "Required": 1,
          "DefaultValue": "",
          "Description": "[{\"id\":\"标记ID,用户自定义标记唯一ID\",\"latitude\":\"纬度\",\"longitude\":\"经度\",\"text\":\"文字标注的内容\",\"fontColor\":\"字体颜色\",\"fontSize\":\"字体大小\"},...]，其中id必须唯一"
        }
      ]
    },
    {
      "ID": "removeAll",
      "Name": "移除所有标记",
      "Description": "移除所有标记",
      "ReturnType": "",
      "ReturnDesc": "",
      "Paras": []
    },
    {
      "ID": "removeMarker",
      "Name": "移除一组指定标记",
      "Description": "移除一组指定标记",
      "ReturnType": "",
      "ReturnDesc": "",
      "Paras": [
        {
          "ID": "ids",
          "Name": "标记ID数组",
          "Type": "Node",
          "Required": 1,
          "DefaultValue": "",
          "Description": "要移除的标记ID数组"
        }
      ]
    },
    {
      "ID": "showPanoramaView",
      "Name": "显示全景图",
      "Description": "根据经纬度显示全景图",
      "ReturnType": "",
      "ReturnDesc": "",
      "Paras": [
        {
          "ID": "latitude",
          "Name": "纬度",
          "Type": "String",
          "Required": 1,
          "DefaultValue": "",
          "Description": "坐标纬度"
        },
        {
          "ID": "longitude",
          "Name": "经度",
          "Type": "String",
          "Required": 1,
          "DefaultValue": "",
          "Description": "坐标经度"
        }
      ]
    }
  ],
  "asyncMethodes": []
}