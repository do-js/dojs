{
  "ID": "do_Bitmap",
  "IsContainer": "0",
  "Type": "MM",
  "Name": "位图",
  "Version": "Internal",
  "Description": "图片的处理组件，通过加载source对应路径的图片到内存中，不再需要使用该位图资源必须调用MM组件基类方法release()来释放该使用内存",
  "Icon": "",
  "IOS": {
    "Support": 1,
    "MinVersion": "7.0"
  },
  "Android": {
    "Support": 1,
    "MinVersion": "4.0"
  },
  "Windows": {
    "Support": 1,
    "MinVersion": "10"
  },
  "WindowsPhone": {
    "Support": 1,
    "MinVersion": "10"
  },
  "Html": {
    "Support": 1,
    "MinVersion": "5.0"
  },
  "Properties": [],
  "Events": [],
  "syncMethodes": [],
  "asyncMethodes": [
    {
      "ID": "loadFile",
      "Name": "加载位图",
      "Description": "加载位图到内存中",
      "ReturnType": "Bool",
      "ReturnType_JavaScript": "Bool",
      "ReturnDesc": "成功返回true，失败返回false",
      "Paras": [
        {
          "ID": "source",
          "Name": "图片路径",
          "Type": "String",
          "Type_JavaScript": "String",
          "Required": 1,
          "DefaultValue": "",
          "Description": "可设置网络或本地文件，支持：http:// https:// data:// source:// 打头的URI格式，不能包含@符号。其中文件格式说明可参考Storage类"
        }
      ]
    },
    {
      "ID": "save",
      "Name": "保存位图",
      "Description": "保存位图到本地",
      "ReturnType": "String",
      "ReturnType_JavaScript": "String",
      "ReturnDesc": "返回当前保存的图片路径，为空时表示保存失败",
      "Paras": [
        {
          "ID": "format",
          "Name": "压缩格式",
          "Type": "String",
          "Type_JavaScript": "String",
          "Required": 0,
          "DefaultValue": "JPEG",
          "Description": "支持两种格式：PNG，JPEG"
        },
        {
          "ID": "quality",
          "Name": "压缩质量",
          "Type": "Number",
          "Type_JavaScript": "Number",
          "Required": 0,
          "DefaultValue": "100",
          "Description": "图片的压缩质量，支持 1-100，windows平台不支持"
        },
        {
          "ID": "outPath",
          "Name": "输出路径",
          "Type": "String",
          "Type_JavaScript": "String",
          "Required": 0,
          "DefaultValue": "",
          "Description": "保存的图片路径支持：data://开头，如果为空，缺省返回唯一图片路径，会另存到data://temp/do_Bitmap/目录下"
        }
      ]
    },
    {
      "ID": "toFrostedGlass",
      "Name": "转成毛玻璃位图",
      "Description": "转成毛玻璃位图",
      "ReturnType": "Bool",
      "ReturnType_JavaScript": "Bool",
      "ReturnDesc": "成功返回true，失败返回false",
      "Paras": [
        {
          "ID": "degree",
          "Name": "模糊程度",
          "Type": "Number",
          "Type_JavaScript": "Number",
          "Required": 1,
          "DefaultValue": "",
          "Description": "图片的模糊程度，支持 1-100"
        }
      ]
    },
    {
      "ID": "toGrayScale",
      "Name": "转成灰色位图",
      "Description": "转换成灰色位图",
      "ReturnType": "Bool",
      "ReturnType_JavaScript": "Bool",
      "ReturnDesc": "成功返回true，失败返回false",
      "Paras": []
    },
    {
      "ID": "toRoundCorner",
      "Name": "添加圆角",
      "Description": "为位图添加圆角效果，windows平台不支持",
      "ReturnType": "Bool",
      "ReturnType_JavaScript": "Bool",
      "ReturnDesc": "成功返回true，失败返回false",
      "Paras": [
        {
          "ID": "radius",
          "Name": "圆角半径",
          "Type": "Number",
          "Type_JavaScript": "Number",
          "Required": 1,
          "DefaultValue": "",
          "Description": ""
        }
      ]
    }
  ]
}